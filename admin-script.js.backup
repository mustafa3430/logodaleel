function updateActiveNavItem(pageName) {
    // Remove active class from all nav items
    const navItems = document.querySelectorAll('.nav-item');
    navItems.forEach(item => {
        item.classList.remove('active');
    });
    
    // Add active class to the current nav item
    const activeNavItem = Array.from(navItems).find(item => 
        item.querySelector('span').textContent === pageName
    );
    if (activeNavItem) {
        activeNavItem.classList.add('active');
    }
    
    console.log(`üìÑ Navigated to: ${pageName}`);
}

// URL Fragment Support Functions
function getPageFromFragment() {
    const hash = window.location.hash.substring(1); // Remove #
    if (!hash) return null;
    
    // Map fragment to pageId
    const fragmentToPage = {
        'dashboard': 'dashboardPage',
        'reports': 'reportsPage',
        'categories': 'categoriesPage',
        'blacklist': 'blacklistPage',
        'archive': 'archivePage',
        'sitesettings': 'siteSettingsPage',
        'settings': 'siteSettingsPage' // Alternative
    };
    
    return fragmentToPage[hash.toLowerCase()] || null;
}

function initializePageFromURL() {
    // Check URL fragment first
    const fragmentPage = getPageFromFragment();
    if (fragmentPage) {
        console.log('üìç Restoring page from URL fragment:', fragmentPage);
        showPage(fragmentPage);
        return true;
    }
    
    // Fallback to localStorage
    const savedPage = localStorage.getItem('logodaleel_current_page');
    if (savedPage && document.getElementById(savedPage)) {
        console.log('üìç Restoring page from localStorage:', savedPage);
        showPage(savedPage);
        return true;
    }
    
    return false; // Use default
}

// Handle browser back/forward with fragments
function handleHashChange() {
    const fragmentPage = getPageFromFragment();
    if (fragmentPage) {
        const currentPage = document.querySelector('.page.active');
        const currentPageId = currentPage ? currentPage.id : null;
        
        if (currentPageId !== fragmentPage) {
            console.log('üîÑ Hash changed, switching to:', fragmentPage);
            showPage(fragmentPage);
            
            // Update nav menu visual state
            const pageToNav = {
                'dashboardPage': 'Dashboard',
                'reportsPage': 'Reports',
                'categoriesPage': 'Categories',
                'blacklistPage': 'Blacklist',
                'archivePage': 'Archive',
                'siteSettingsPage': 'Site Settings'
            };
            
            if (pageToNav[fragmentPage]) {
                updateActiveNavItem(pageToNav[fragmentPage]);
            }
        }
    }
}

