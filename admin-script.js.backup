    }
    
    if (typeof category === 'object') {
        return {
            level1_en: category.level1?.en || category.name?.split(' > ')[0] || category.level1 || '',
            level1_ar: category.level1?.ar || category.nameAr?.split(' > ')[0] || category.level1Ar || '',
