function showPage(pageId) {
    // Hide all pages
    const pages = document.querySelectorAll('.page');
    pages.forEach(page => {
        page.classList.remove('active');
    });
    
    // Show the selected page
    const targetPage = document.getElementById(pageId);
    if (targetPage) {
        setTimeout(() => {
            targetPage.classList.add('active');
        }, 50); // Small delay for smooth transition
        
        // Save current page to localStorage for browser refresh persistence
        saveCurrentPage(pageId);
        
        // Update URL fragment for bookmarking (without triggering reload)
        const fragment = pageId.replace('Page', '').toLowerCase();
        if (window.location.hash !== '#' + fragment) {
            history.replaceState(null, null, '#' + fragment);
        }
    }
    
    // Show/hide context-sensitive buttons based on page
    const addCompanyBtn = document.getElementById('addCompanyBtn');
    const exportDataBtn = document.getElementById('exportDataBtn');
    
    if (addCompanyBtn) {
        if (pageId === 'dashboardPage') {
            addCompanyBtn.style.display = 'flex';
        } else {
            addCompanyBtn.style.display = 'none';
        }
    }
    
    if (exportDataBtn) {
        if (pageId === 'dashboardPage' || pageId === 'reportsPage') {
            exportDataBtn.style.display = 'flex';
        } else {
            exportDataBtn.style.display = 'none';
        }
    }
}

// Save the current page to localStorage
function saveCurrentPage(pageId) {
    try {
        localStorage.setItem('logodaleel_current_page', pageId);
        console.log(`üíæ Current page saved: ${pageId}`);
    } catch (error) {
        console.warn('‚ö†Ô∏è Could not save current page:', error);
    }
}

// Restore the last visited page after browser refresh
function restoreCurrentPage() {
    try {
        const savedPage = localStorage.getItem('logodaleel_current_page');
        if (savedPage && document.getElementById(savedPage)) {
            console.log(`üîÑ Restoring page after browser refresh: ${savedPage}`);
            showPage(savedPage);
            
            // Update the navigation item based on page
            const pageToNavMap = {
                'dashboardPage': 'Dashboard',
                'reportsPage': 'Reports',
                'categoriesPage': 'Categories',
                'blacklistPage': 'Blacklist',
                'archivePage': 'Archive',
                'siteSettingsPage': 'Site Settings'
            };
            
            const navName = pageToNavMap[savedPage] || 'Dashboard';
            updateActiveNavItem(navName);
            
            // Load data for the restored page
            loadPageData(savedPage);
            
            console.log(`‚úÖ Successfully restored to ${navName} page`);
            return true; // Successfully restored
        } else {
            console.log('üìÑ No saved page found or page element missing, defaulting to dashboard');
            return false; // No saved page, use default
        }
    } catch (error) {
        console.warn('‚ö†Ô∏è Could not restore current page:', error);
        return false; // Error, use default
    }
}

